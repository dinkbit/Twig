{
    "name": "dinkbit/twig",
    "description": "Simple and flexible Laravel integration with Twig template engine.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "twig"
    ],
    "authors": [
        {
            "name": "Joseph Cohen",
            "email": "joseph.cohen@dinkbit.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.0",
        "twig/twig": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Dinkbit\\Twig\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dinkbit\\Twig\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
